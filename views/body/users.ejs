<div>
    <table id="usersTable" class="table table-bordred table-striped">

        <thead><tr>
            <th><input type="checkbox" id="checkall"></th>
            <%
            props.forEach((fieldName) => {
            %><th><%=fieldName%></th><%
            })
            if (editable) {%>
            <th>Edit</th>
            <th>Delete</th>
            <%} %>
        </tr></thead>
        <tbody>

        <% users.forEach((obj, rowIndex) => { %>
        <tr>
            <td><input type="checkbox" class="checkthis"></td>
            <% props.forEach(field => { %>
            <td><%=obj[field]%></td>
            <%
            });
            if (editable)
            {%>
            <td><p data-placement="top" data-toggle="tooltip" title="" data-original-title="Edit"><button class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit" onclick="onSelectId(<%=obj.id%>)"><span class="glyphicon glyphicon-pencil"></span></button></p></td>
            <td><p data-placement="top" data-toggle="tooltip" title="" data-original-title="Delete"><button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" onclick="onSelectId(<%=obj.id%>)"><span class="glyphicon glyphicon-trash"></span></button></p></td>
            <%}%>
        </tr>
        <%}) %>

        </tbody>

    </table>


    <button type="button" class="btn btn-danger bmd-btn-fab">
        <i class="material-icons">grade</i>
    </button>

    <!-- Modal -->
    <div id="delete" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Delete</h4>
                </div>
                <div class="modal-body">
                    <p id="#delete-modal-text" >Are you sure you want to delete user id <span class="selected-id"></span>?</p>
                </div>
                <div class="modal-footer">
                    <button id="delete-button-delete" type="button" class="btn btn-default" data-dismiss="modal" onclick="onDeleteClick()">Delete</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                </div>
            </div>

        </div>
    </div>


</div>